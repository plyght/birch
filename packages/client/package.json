{
  "name": "@inaplight/birch-client",
  "version": "0.1.0",
  "description": "Zero-config automatic API key rotation client for Birch",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./auto": {
      "types": "./dist/auto.d.ts",
      "default": "./dist/auto.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "birch",
    "secret-rotation",
    "api-key",
    "rate-limit",
    "429",
    "automatic-rotation"
  ],
  "author": "Birch Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/plyght/birch.git",
    "directory": "packages/client"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}

